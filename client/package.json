{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://g3f4-lifeapp.herokuapp.com/graphql/",
  "dependencies": {
    "babel-plugin-relay": "^1.3.0",
    "material-ui": "^0.19.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-relay": "^1.3.0",
    "react-scripts": "1.0.13",
    "relay-compiler": "^1.3.0",
    "relay-runtime": "^1.3.0",
    "shallowequal": "^1.0.2"
  },
  "scripts": {
    "build": "node setup && react-scripts build && cp -r build/* ../build",
    "heroku:build": "node setup && REACT_APP_GRAPHQL_HOST=https://g3f4-lifeapp.herokuapp.com/graphql react-scripts build && cp -r build/* ../build",
    "test": "node setup && react-scripts test --env=jsdom",
    "start": "node setup && react-scripts start",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ../data/schema.graphql --extensions=jsx --extensions=js --watch"
  }
}

{
  "name": "lifeapp",
  "version": "0.3.0",
  "description": "Level up with LifeApp",
  "main": "lifeapp",
  "scripts": {
    "build": "npm run build:server && npm run build:client && cp package.json build/",
    "build:client": "cd client && node setup && react-scripts build && cp -r build/* ../build && cd -",
    "build:server": "webpack --env production --config webpack.server.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "dev": "BABEL_ENV=server babel-watch server",
    "start": "npm run build && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 build/server",
    "stats": "webpack --env production --profile --json > stats.json",
    "test": "node setup && react-scripts test --env=jsdom",
    "update:mocks": "BABEL_ENV=scripts babel-node ./scripts/updateMocks",
    "update:schema": "BABEL_ENV=scripts babel-node ./scripts/updateSchema"
  },
  "keywords": [],
  "author": "G3F4.XYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g3f4-xyz/lifeapp.git"
  },
  "bugs": {
    "url": "https://github.com/g3f4-xyz/lifeapp/issues"
  },
  "homepage": "https://github.com/g3f4-xyz/lifeapp#readme",
  "devDependencies": {
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-relay": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "babel-watch": "^2.0.6",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-graphql": "^0.6.11",
    "faker": "^4.1.0",
    "graphql": "^0.11.3",
    "graphql-relay": "^0.5.1",
    "immutability-helper": "^2.3.1",
    "json-loader": "^0.5.7",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-scripts": "1.0.13",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  }
}

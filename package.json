{
  "name": "lifeapp",
  "version": "0.2.0",
  "description": "Level up with LifeApp",
  "main": "lifeapp",
  "scripts": {
    "build": "webpack --env production",
    "build:client": "webpack --env production --config webpack.client.js",
    "build:server": "webpack --env production --config webpack.server.js",
    "client": "GRAPHQL_PORT=30000 webpack-dev-server --env development --config webpack.client.js",
    "lint:css": "stylelint app/**/*.css",
    "lint:js": "eslint . --cache",
    "server": "BABEL_ENV=server babel-watch server",
    "start": "npm --optimize_for_size --max_old_space_size=460 --gc_interval=100 run build && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 build/server",
    "stats": "webpack --env production --profile --json > stats.json",
    "test": "mocha './{,!(node_modules)/**/}*.test.js'",
    "update:mocks": "BABEL_ENV=scripts babel-node ./scripts/updateMocks",
    "update:schema": "BABEL_ENV=scripts babel-node ./scripts/updateSchema"
  },
  "keywords": [],
  "author": "G3F4.XYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g3f4-xyz/lifeapp.git"
  },
  "bugs": {
    "url": "https://github.com/g3f4-xyz/lifeapp/issues"
  },
  "homepage": "https://github.com/g3f4-xyz/lifeapp#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-relay-plugin": "^0.11.0",
    "babel-runtime": "^6.23.0",
    "babel-watch": "^2.0.6",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "http-server": "^0.9.0",
    "mocha": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "purifycss-webpack": "^0.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "express": "^4.14.1",
    "express-graphql": "^0.6.3",
    "faker": "^4.1.0",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-motion": "^0.4.7",
    "react-relay": "^0.10.0",
    "react-tap-event-plugin": "^2.0.1",
    "shallowequal": "^0.2.2"
